		<queryString>
			<![CDATA[select t2.detail_no,' '+ISNULL(t1.spec_field_name,'') spec_field_name,' '+ISNULL(t1.spec_field,'') spec_field from
(
select a.detail_no,a.spec_field_name,a.spec_field from ZT_Pay_Detail_List a
where a.detail_type='2' and pay_detail_uuid=$P{pay_detail_uuid}
) t1 right join
(
select 1 detail_no
union all
select 2 detail_no
union all
select 3 detail_no
union all
select 4 detail_no
union all
select 5 detail_no
union all
select 6 detail_no
union all
select 7 detail_no
union all
select 8 detail_no
union all
select 9 detail_no
union all
select 10 detail_no
) t2 on t1.detail_no=t2.detail_no where t2.detail_no<=4]]>
		</queryString>
		<field name="detail_no" class="java.lang.Integer"/>
		<field name="spec_field_name" class="java.lang.String"/>
		<field name="spec_field" class="java.lang.String"/>
