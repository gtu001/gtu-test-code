	<parameter name="pay_detail_uuid" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="scissor" class="java.lang.String"/>
	<queryString>
		<![CDATA[select
zm.PaymentName,
b1.pay_detail_uuid,
  b1.pay_uuid,
  b1.serial_num,
  b1.uniformno,
  b1.company_name,
  b1.company_address,
  b1.company_tel,
  ''+ISNULL(b1.pay_code,'') pay_code,
  ''+ISNULL(b1.pay_name,'') pay_name,
  b1.pay_post,
  b1.pay_address,
  b1.pay_email,
  b1.deadline_time,
  b1.barcode_agent_receive_time,
  b1.notice,
  b1.remark,
  b1.spec_code_name1,
  b1.spec_code_name2,
  b1.spec_code_value1,
  b1.spec_code_value2,
  b1.bank_agent_fee,
  b1.bank_barcode1,
  b1.bank_barcode2,
  b1.post_agent_fee,
  b1.post_barcode1,
  b1.post_barcode2,
  b1.post_barcode3,
  b1.store_agent_fee,
  b1.store_barcode1,
  b1.store_barcode2,
  b1.store_barcode3,
  b1.[status],
  b1.error_status,
  b1.pay_fee,
  b1.pay_receive_fee,
  b1.agent_receive_fee,
  b1.agent_chcode,
  b1.modify_id,
  b1.modify_name,
  b1.modify_time,
  b1.is_reported,
  b1.pin_account_code,
  b1.pin_account_date,
  b1.DailyIndex,
case when REPLACE(b1.bank_barcode1,' ','')='' then Null else b1.bank_barcode1 end as bank_barcode1_d,
case when REPLACE(b1.bank_barcode2,' ','')='' then Null else b1.bank_barcode2 end as bank_barcode2_d,
case when REPLACE(b1.post_barcode1,' ','')='' then Null else b1.post_barcode1 end as post_barcode1_d,
case when REPLACE(b1.post_barcode2,' ','')='' then Null else b1.post_barcode2 end as post_barcode2_d,
case when REPLACE(b1.post_barcode3,' ','')='' then Null else b1.post_barcode3 end as post_barcode3_d,
case when REPLACE(b1.store_barcode1,' ','')='' then Null else b1.store_barcode1 end as store_barcode1_d,
case when REPLACE(b1.store_barcode2,' ','')='' then Null else b1.store_barcode2 end as store_barcode2_d,
case when REPLACE(b1.store_barcode3,' ','')='' then Null else b1.store_barcode3 end as store_barcode3_d,
convert(varchar(3),(convert(int,datepart(year,b1.deadline_time))-1911))
+'年'
+convert(varchar(2),datepart(month,b1.deadline_time))
+'月'
+convert(varchar(2),datepart(day,b1.deadline_time))
+'日止' deadline_time_M,
convert(decimal,right(case when REPLACE(b1.store_barcode3,' ','')='' then Null else b1.store_barcode3 end,9)) store_barcode3_fee,
'*'+b1.store_barcode1+'*' store_barcode1_M ,
'*'+b1.store_barcode2+'*' store_barcode2_M ,
'*'+b1.store_barcode3+'*' store_barcode3_M ,
convert(decimal,case when REPLACE(b1.bank_barcode2,' ','')='' then Null else b1.bank_barcode2 end) bank_barcode2_M,
'【'+reverse(stuff(reverse(convert(varchar,convert(money,case when REPLACE(b1.bank_barcode2,' ','')='' then Null else b1.bank_barcode2 end),1)),1,3,'')) +'】' bank_barcode2_M1,
'【'+case when REPLACE(b1.bank_barcode1,' ','')='' then Null else b1.bank_barcode1 end+'】' bank_barcode1_M1,
convert(decimal,case when REPLACE(b1.post_barcode3,' ','')='' then Null else b1.post_barcode3 end) post_barcode3_M,
ISNULL(a1.notice,'')  msg,
ISNULL(b1.pay_name,'')+'   ' pay_name_M,
'('+company_name+'代收專戶)' company_name_M,
(
	select case count(*) when 0 then 'N' else 'Y' end bank from
	(
		select c.* from ZT_Pay_Master a,ZT_Pay_Detail b,ZT_BillerMain c
		where a.pay_uuid=b.pay_uuid and a.biller_main_uuid=c.BillerMain_uuid
		and b.pay_detail_uuid=$P{pay_detail_uuid}
	) t1 where t1.IsBankChainGroup = '1'
) bankDisplay,
(
	select case count(*) when 0 then 'N' else 'Y' end store from
	(
		select c.* from ZT_Pay_Master a,ZT_Pay_Detail b,ZT_BillerMain c
		where a.pay_uuid=b.pay_uuid and a.biller_main_uuid=c.BillerMain_uuid
		and b.pay_detail_uuid=$P{pay_detail_uuid} and ISNULL(b.store_barcode1,'')!=''
	) t1 where t1.IsStoreChainGroup = '1'
) storeDisplay,
(
	select case count(*) when 0 then 'N' else 'Y' end post from
	(
		select c.* from ZT_Pay_Master a,ZT_Pay_Detail b,ZT_BillerMain c
		where a.pay_uuid=b.pay_uuid and a.biller_main_uuid=c.BillerMain_uuid
		and b.pay_detail_uuid=$P{pay_detail_uuid} and ISNULL(b.post_barcode1,'')!=''
	) t1 where t1.IsPostChainGroup = '1'
) postDisplay,
(
	select case count(*) when 0 then 'N' else 'Y' end bank from
	(
		select c.* from ZT_Pay_Master a,ZT_Pay_Detail b,ZT_BillerMain c
		where a.pay_uuid=b.pay_uuid and a.biller_main_uuid=c.BillerMain_uuid
		and b.pay_detail_uuid=$P{pay_detail_uuid}
	) t1 where t1.isATM = '1'
) bankDisplay_ATM,
(
	select case count(*) when 0 then 'N' else 'Y' end store from
	(
		select c.* from ZT_Pay_Master a,ZT_Pay_Detail b,ZT_BillerMain c
		where a.pay_uuid=b.pay_uuid and a.biller_main_uuid=c.BillerMain_uuid
		and b.pay_detail_uuid=$P{pay_detail_uuid} and ISNULL(b.bank_barcode1,'')!=''
	) t1 where t1.isCTCB = '1'
) bankDisplay_D00,
(
	select case count(*) when 0 then 'N' else 'Y' end bank from
	(
		select c.* from ZT_Pay_Master a,ZT_Pay_Detail b,ZT_BillerMain c
		where a.pay_uuid=b.pay_uuid and a.biller_main_uuid=c.BillerMain_uuid
		and b.pay_detail_uuid=$P{pay_detail_uuid} and ISNULL(b.bank_barcode1,'')!=''
	) t1 where t1.isATM = '1'
) atmDisplay

from ZT_Pay_Master a1,ZT_Pay_Detail b1,ZT_BillerMain zm where a1.pay_uuid=b1.pay_uuid and a1.biller_main_uuid=zm.BillerMain_uuid and b1.pay_detail_uuid=$P{pay_detail_uuid}]]>
	</queryString>
