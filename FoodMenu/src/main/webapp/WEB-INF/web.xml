<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0">
	<servlet>
		<servlet-name>AjaxSetupServlet</servlet-name>
		<servlet-class>com.janna.servlet.AjaxSetupServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AjaxSetupServlet</servlet-name>
		<url-pattern>/AjaxSetupServlet</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>AjaxUploadServlet</servlet-name>
		<servlet-class>com.janna.servlet.AjaxUploadServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AjaxUploadServlet</servlet-name>
		<url-pattern>/AjaxUploadServlet</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>DispatcherServlet</servlet-name>
		<servlet-class>com.janna.servlet.DispatcherServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DispatcherServlet</servlet-name>
		<url-pattern>/DispatcherServlet</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>DownloadServlet</servlet-name>
		<servlet-class>com.janna.servlet.DownloadServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DownloadServlet</servlet-name>
		<url-pattern>/DownloadServlet</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>JqGridServlet</servlet-name>
		<servlet-class>com.janna.servlet.JqGridServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>JqGridServlet</servlet-name>
		<url-pattern>/JqGridServlet</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>OrderServlet</servlet-name>
		<servlet-class>com.janna.servlet.OrderServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>OrderServlet</servlet-name>
		<url-pattern>/OrderServlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>HtmlJavascriptEscapeServlet</servlet-name>
		<servlet-class>com.janna.servlet.HtmlJavascriptEscapeServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>HtmlJavascriptEscapeServlet</servlet-name>
		<url-pattern>/HtmlJavascriptEscapeServlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>DebugMointerUIHotServlet</servlet-name>
		<servlet-class>gtu.log.finder.etc.DebugMointerUIHotServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DebugMointerUIHotServlet</servlet-name>
		<url-pattern>/debug</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
</web-app>